---
export interface Props {
  title: string;
}

const { title } = Astro.props;
---

<html lang="ko">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{title}</title>
    <link rel="stylesheet" href="/assets/icons.css" />
  </head>
  <body>
    <slot />
  </body>
</html>
